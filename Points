LED ON : 
	echo 73 > /sys/class/gpio/export
	echo out > /sys/class/gpio/gpio73/direction
	ON :  echo 1 > /sys/class/gpio/gpio73/value	
	OFF : echo 0 > /sys/class/gpio/gpio73/value

